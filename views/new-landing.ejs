	<div style="width: 100%; background-color: #49274a; color: white;" class="card landing-add mt-5">
		<h5 class="ml-4 mt-2 mb-2" style="text-align: center;">TEXT</h5>
	</div>

	<div id="showtext">
		<%include ./ajax/text%>
