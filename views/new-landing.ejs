<% include ./partials/header %>

	<div id="carouselExampleIndicators" class="carousel slide top" data-ride="carousel">
	  <ol class="carousel-indicators">
	    <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
	    <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
	    <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
	    <li data-target="#carouselExampleIndicators" data-slide-to="3"></li>
	    <li data-target="#carouselExampleIndicators" data-slide-to="4"></li>
	  </ol>
	  <div class="carousel-inner">
	    <div class="carousel-item active">
	      <img class="d-block w-100 carouselimage" src="/assets/images/img1.jpg" alt="First slide">
	    </div>
	    <div class="carousel-item">
	      <img class="d-block w-100 carouselimage" src="/assets/images/img2.jpg" alt="Second slide">
	    </div>
	    <div class="carousel-item">
	      <img class="d-block w-100 carouselimage" src="/assets/images/img3.jpg" alt="Third slide">
	    </div>
	    <div class="carousel-item">
	      <img class="d-block w-100 carouselimage" src="/assets/images/img4.jpg" alt="Third slide">
	    </div>
	    <div class="carousel-item">
	      <img class="d-block w-100 carouselimage" src="/assets/images/img5.jpg" alt="Third slide">
	    </div>
	  </div>
	  <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
	    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
	    <span class="sr-only">Previous</span>
	  </a>
	  <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
	    <span class="carousel-control-next-icon" aria-hidden="true"></span>
	    <span class="sr-only">Next</span>
	  </a>
	</div>

	<div style="width: 60rem;" class="card landing-add mt-4">
		<img style="height: 10rem;" src="/img/banners/landing_add.jpg">
	</div>

	<div style="width: 100%; background-color: #49274a; color: white;" class="card landing-add mt-4">
		<h5 class="ml-4 mt-2 mb-2" style="text-align: center;">Customize your own design</h5>
	</div>

	<div style="width: 60rem;" class="landing-add mt-5">
		<div class="row">
			<div class="col-lg-6 col-md-6 col-6 ">
				<img class="card-1" style="height: 19rem; cursor: pointer;" src="/img/banners/customDesign.jpg">
			</div>
			<div class="col-lg-6 col-md-6 col-6">
				<img class="card-1" style="height: 19rem; cursor: pointer;" src="/img/banners/customDesign.jpg">
			</div>
		</div>
	</div>

	<div style="width: 100%; background-color: #49274a; color: white;" class="card landing-add mt-5">
		<h5 class="ml-4 mt-2 mb-2" style="text-align: center;">ALL T-SHIRTS</h5>
	</div>

	<!-- types of category -->
	<div id="showAllshirts">
		<%include ./ajax/allShirts%>
	</div>
	
	<div style="width: 100%; background-color: #49274a; color: white;" class="card landing-add mt-5">
		<h5 class="ml-4 mt-2 mb-2" style="text-align: center;">TRENDING</h5>
	</div>

	<div id="showtrending">
		<%include ./ajax/trending%>
